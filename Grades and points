point = int(input("How many points [0-100]: "))
if point > 100:
    print("Grade: impossible!")
elif point >= 90 and point <= 100:
    print("Grade: 5")
elif point >= 80:
    print("Grade: 4")
elif point >= 70:
    print("Grade: 3")
elif point >= 60:
    print("Grade: 2")
elif point >= 50:
    print("Grade: 1")
elif point < 50 and point >=0 :
    print("Grade: fail")
elif point < 0:
    print("Grade: impossible!")
